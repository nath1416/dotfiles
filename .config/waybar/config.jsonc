{
  "layer": "top",
  "height": 25,
  "spacing": 4,
  "reload_style_on_change": true,
  "modules-left": [
    "hyprland/workspaces"
  ],
  "modules-center": [
    "clock"
  ],
  "modules-right": [
    "group/expand",
    "custom/profile",
    "network",
    "bluetooth",
    "wireplumber",
//    "custom/profile",
    "memory",
    "cpu",
    "battery"
  ],
  "group/expand": {
    "orientation": "horizontal",
    "drawer": {
      "transition-duration": 600,
      "transition-to-left": true,
      "click-to-reveal": true
    },
    "modules": [
      "custom/expand",
      "tray",
      "backlight",
      "mpris",
      "custom/endpoint"
    ]
  },
  "custom/expand": {
    "format": "ÔÑÑ",
    "tooltip": false
  },
"custom/profile": {
    "format": "{}",
    "exec": "cat /sys/firmware/acpi/platform_profile",
    "interval": 10,
    "tooltip-format": "To change power profile:\nFn+l: Low-Power\nFn+m: Balanced\nFn+h: Performance"
},
  "custom/endpoint": {
    "format": "|",
    "tooltip": false
  },
  "hyprland/workspaces": {
    "format": "{name}",
    "show-special": true
  },
  "keyboard-state": {
    "numlock": true,
    "capslock": true,
    "format": "{name} {icon}",
    "format-icons": {
      "locked": "ÔÄ£",
      "unlocked": "ÔÇú"
    }
  },
  "tray": {
    "spacing": 10
  },
  "clock": {
    "format": "{:%a %d %R} ",
    "format-alt": "{:%H:%M}",
    "tooltip-format": "<big>{:%Y %B}</big>\n<tt><small>{calendar}</small></tt>"
  },
  "cpu": {
    "format": "{usage}% Ôãõ",
    "format-alt": "{usage0}%, {usage1}%, {usage2}%, {usage3}% Ôãõ",
    "tooltip-format": "sdhf"
  },
  "memory": {
    "format": "{}% ÓøÖ",
    "format-alt": "{used:0.1f}G/{total:0.1f}G ÓøÖ"
  },
  "bluetooth": {
    "format-on": "Û∞ÇØ",
    "format-off": "BT-off", 
    "format-disabled": "Û∞Ç≤",
    "format-connected-battery": "Û∞ÇØ {device_alias}-{device_battery_percentage}% Û∞ÇÇ",
//    "format-alt": "{device_alias}Û∞ÇØ",
    "tooltip-format": "{controller_alias}\t{controller_address}\n\n{num_connections} connected",
    "tooltip-format-connected": "{controller_alias}\n\n{num_connections} connected\n\n{device_enumerate}",
    "tooltip-format-enumerate-connected": "{device_alias}\n{device_address}",
    "tooltip-format-enumerate-connected-battery": "{device_alias}\n Battery: {device_battery_percentage}%",
    "on-click": "blueman-manager"
  },
  "battery": {
    "states": {
      "good": 90,
      "warning": 30,
      "critical": 15
    },
    "format": "{capacity}% {icon}",
    "format-full": "{capacity}% {icon}",
    "format-charging": "{capacity}% Û∞ÇÖ",
    "format-plugged": "{capacity}% Û∞ÅπÔá¶",
    "format-alt": "{time} {icon}",
    "format-icons": [
      "Û∞Åª",
      "Û∞ÅΩ",
      "Û∞Åø",
      "Û∞ÇÅ",
      "Û∞Åπ"
    ]
  },
  "network": {
    "format-wifi": " Ôá´  {essid} ",
    "format-ethernet": "Û∞àÄ {ifname} ",
    "format-disconnected": "Disconnected ‚ö†",
    "tooltip-format": "Ip\t{ipaddr}\nUp\t{bandwidthUpBits}\nDown\t{bandwidthDownBits}",
    "tooltip-format-wifi": "Ip\t{ipaddr}\nUp\t{bandwidthUpBits}\nDown\t{bandwidthDownBits}\nSignal\t{signalStrength}%\nFreq\t{frequency}GHz",
    "on-click": ""
  },
  "wireplumber": {
    "scroll-step": "5.0",
    "format": "{volume}% Û∞ìÉ",
    "format-muted": "Û∞ùü",
    "on-click": "pwvucontrol"
  },
  "mpris": {
	"format": "{player_icon} {album}-{artist}",
	"format-paused": "{status_icon} {album}-{artist}",
	"player-icons": {
		"default": "‚ñ∂",
		"mpv": "üéµ"
	},
	"status-icons": {
		"paused": "‚è∏"
	}
	// "ignored-players": ["firefox"]
}

}
